pipeline {
  agent any
  {
    stages
    {
      stage("build"){
        steps{
          echo 'build the application'
        }
      }
      stage("Test"){
        steps{
          echo 'Test is completed for the application'
        }
      }
      stage("Deploy"){
        steps{
          echo 'deploying the application'
        }
      }
    }
  }
}
